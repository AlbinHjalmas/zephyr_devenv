{
    "configurations": [
        {
            "name": "firmware Debug",
            "includePath": [
                "${workspaceFolder}/firmware/src/**"
            ],
            "defines": [
                "__CDT_PARSER__=1",
                "__ASSERT_ON=1"
            ],
            "compilerPath": "/opt/toolchains/zephyr-sdk-0.16.3/arm-zephyr-eabi/bin/arm-zephyr-eabi-gcc",
            "cppStandard": "${default}",
            "cStandard": "${default}",
            "intelliSenseMode": "gcc-arm",
            "compileCommands": "${workspaceFolder}/firmware/build/compile_commands.json"
        }
    ],
    "version": 4
}