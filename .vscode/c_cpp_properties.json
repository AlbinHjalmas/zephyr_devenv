{
    "configurations": [
        {
            "name": "firmware Debug",
            "includePath": [
                "${workspaceFolder}/firmware/src/**",
                "${workspaceFolder}/zerv/include/**"
            ],
            "defines": [
                "__ASSERT_ON=1",
                "__CDT_PARSER__"
            ],
            "compilerPath": "/opt/toolchains/zephyr-sdk-0.16.3/arm-zephyr-eabi/bin/arm-zephyr-eabi-gcc",
            "cppStandard": "${default}",
            "cStandard": "${default}",
            "intelliSenseMode": "gcc-arm",
            "compileCommands": "${workspaceFolder}/firmware/build/compile_commands.json"
        },
        {
            "name": "libloragw",
            "includePath": [
                "${workspaceFolder}/lora-gateway/sx1302_hal/libloragw/inc/**",
            ],
            "compilerPath": "/usr/bin/gcc",
            "cppStandard": "${default}",
            "cStandard": "${default}",
            "intelliSenseMode": "linux-gcc-x64",
        }
    ],
    "version": 4
}