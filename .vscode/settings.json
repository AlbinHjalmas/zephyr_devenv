{
    "files.associations": {
        "socket.h": "c",
        "net_if.h": "c"
    },
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "clang-format.executable": "/usr/local/bin/clang-format",
    "clang-format.assumeFilename": "/workdir/zephyr/.clang-format",
    "clang-format.style": "file",
    "task.allowAutomaticTasks": "on",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "kconfig.root": "/workdir/project/firmware/firmware/Kconfig",
    "devicetree.defaultBoard": "nucleo_f767zi",
    "nrf-connect.applications": [
        "${workspaceFolder}/firmware"
    ],
    "nrf-connect.deviceProvider": "nrfutil"
}